<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>PartyLine</title>
    <link rel="stylesheet" href="/css/partyline.css">

</head>
<body>
    <div class="sidebar">
        <div class="logo">
            <img src="/images/partyline_logo_horizontal-03.png">
        </div>
        <%- include('partials/partyMenu') %>
        <div class="join">
            <button type="button">Join A New Party</button>
        </div>
    </div>
    <div class="content">
        <div class="topbar">
            <%- include('partials/userBar') %>
            <div class="search">
                <%- include('partials/searchBar') %>
            </div>
        </div>
        <div> <%=firstName%></div>
        <div> <%=lastName%></div>
        <div> <%=email%></div>
        <div> <%=username%></div>
        <div> <%=password%></div>

        <div class="subs">
            <% for ( let i = 0; i < Subs.length; i++ ) { %>
            <div class="subscriptionlist"> 
                <div class="partySubs"> <a href="/party/<%=Subs[i].party_id %>"><%= Subs[i].name %></a></div> 
            </div>
            <% } %>
        </div>


</body>
</html>
